{
    "compilerOptions": {
      "outDir": "./built",
      "allowJs": true,
      "target": "ES2015",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "noEmitOnError": true,
      "noImplicitAny": true,
      "moduleResolution": "Node"
    },
    "include": ["./src/**/*"]
  }